<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square70x70logo src="/images/icon-70x70.png"/>
            <square150x150logo src="/images/icon-150x150.png"/>
            <wide310x150logo src="/images/icon-310x150.png"/>
            <square310x310logo src="/images/icon-310x310.png"/>
            <TileColor>#00f5ff</TileColor>
        </tile>
        <notification>
            <polling-uri src="/notifications/notification1.xml"/>
            <polling-uri2 src="/notifications/notification2.xml"/>
            <polling-uri3 src="/notifications/notification3.xml"/>
            <polling-uri4 src="/notifications/notification4.xml"/>
            <polling-uri5 src="/notifications/notification5.xml"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </notification>
    </msapplication>
</browserconfig> 